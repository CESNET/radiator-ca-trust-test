CREATE DATABASE radiator;
USE radiator;
CREATE TABLE catt (
  timestamp int(11),
  username varchar(64),
  mac varchar(64),
  result set('unknown','CArefused','CAaccepted'),
  PRIMARY KEY (timestamp,username,mac)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
